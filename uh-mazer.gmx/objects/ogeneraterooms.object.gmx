          <argument>
            <kind>1</kind>
            <string>///control the room
randomize();

if gen == true {
   do {
      ranroom = irandom(3);
      roomsgenerated++;
      
      if roomsgenerated &gt;= 10 {
         gen = false;
      }
      
      if ranroom == 0 &amp;&amp; up == false { ///draw room to the top
         up = false;
         down = true;
         left = false;
         right = false;
         
         instance_create (genx + 512, geny + 100, odoortop); ///rough center top
         geny -= 768; ///room height
         instance_create (genx, geny, oroom); ///create room
         instance_create (genx, geny + 704, odoorbottom); ///create door in newroom
      }
      
      if ranroom == 1 &amp;&amp; down == false { ///draw room to the bottom
         
         up = true;
         down = false;
         left = false;
         right = false;
         
         instance_create (genx + 512, geny + 704, odoorbottom);
         geny += 768
         instance_create (genx, geny, oroom);
         instance_create (genx, geny, odoortop);
         
      }
      
      if ranroom == 2 &amp;&amp; right == false { ///generates room to the right
         
         right = false;
         left = true;
         up = false;
         down = false;
         
         instance_create (genx + 960, geny + 352, odoorright);
         genx += 1024
         instance_create (genx, geny, oroom);
         instance_create (genx + 30, geny + 352, odoorleft);
      }
      
      if ranroom == 3 &amp;&amp; left == false { ///generates room to the left
         
         left = false;
         right = true;
         up = false;
         down = false;
         
         instance_create (genx, geny + 352, odoorleft);
         genx += 1024
         instance_create (genx, geny, oroom);
         instance_create (genx, geny + 352, odoorright);
      }
      
   }
   until roomsgenerated &gt;= 10
}
</string>
